@model IEnumerable<ThriveWebApp.Models.Contact>
@{
    ViewBag.Title = "ShowFeedback";
}

@section logged{
    @if (Session["name"] == null || Session["name"].ToString() == "Staff")
    {
        Response.Redirect(Url.Action("Login", "User"));
    }
}

<h2>Customer Feedback</h2>
@ViewData["message"]

@foreach (var contact in Model)
{
    <section>
        <p>User name: <b>@contact.Name</b></p>
        <p>User Email: <b>@contact.Email</b></p>
        <p>@contact.Comments</p>
        <hr/>
    </section>

}